package Mneson.XML is   -- Is_Complex =   --    has both #PCDATA and regular elements as children   --       or   --    has attributes and content.      -- Is_Ordered = children are ordered.   -- Naturally, only complex elements may be ordered.   generic      with function Get return Character;      with package Work is new Mneson.Work (<>);      with function Is_Complex (Element_Name : Vertex) return Boolean;      with function Is_Ordered (Element_Name : Vertex) return Boolean;      Max_Depth : Standard.Natural := 10000;   function Mnesonise_XML_Stream return Vertex;   End_Of_Stream : exception;   XML_Error : exception;   end;